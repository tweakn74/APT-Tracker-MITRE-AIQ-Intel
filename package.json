{
  "name": "apt-tracker-mitre-aiq-intel",
  "version": "1.0.0",
  "description": "Production-ready threat intelligence aggregator and trends dashboard",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "node --test tests/**/*.test.js",
    "lint": "eslint worker/ tests/ scripts/ app.js",
    "lint:fix": "eslint --fix worker/ tests/ scripts/ app.js",
    "format": "prettier --write \"**/*.{js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,json,md}\"",
    "type-check": "tsc --noEmit",
    "kv:export": "node scripts/kv_export.mjs",
    "backup": "node scripts/backup.mjs",
    "prepare": "husky"
  },
  "keywords": [
    "threat-intelligence",
    "mitre-attack",
    "cve",
    "security",
    "cloudflare-worker"
  ],
  "author": "CG",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "eslint": "^9.17.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.95.0"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
